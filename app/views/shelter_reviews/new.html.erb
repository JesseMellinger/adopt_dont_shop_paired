<%= form_tag("/shelters/#{@shelter_id}") do %>
  <%= label_tag "Title" %>
  <%= text_field_tag :title %>

  <%= label_tag "Rating" %>
  <%= text_field_tag :rating %>

  <%= label_tag "Picture" %>
  <%= text_field_tag :picture %>

  <%= label_tag "Content" %>
  <%= text_field_tag :content %>

  <%= label_tag "Name" %>
  <%= text_field_tag :name %>

  <%= submit_tag "Create Review"%>
<% end %>
